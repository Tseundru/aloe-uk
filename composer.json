{
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
              "name": "advanced-custom-fields/advanced-custom-fields-pro",
              "version": "6.0.3",
              "type": "wordpress-plugin",
              "dist": {
                "type": "zip",
                "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download&k=Nzc3ZTY1YjIxZDRjZDBkMDFjOTE5NDQxMTM2OWYyOTFlYWMyZDM1Y2MxOTgyZjNiOTcwM2Jj&t=6.0.3"
