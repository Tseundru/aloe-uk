    "require": {
        "johnpbloch/wordpress": "^6.0",
        "wpackagist-plugin/akismet":"4.1.4",
        "advanced-custom-fields/advanced-custom-fields-pro": "^6.0.3",
        
        "wpackagist-plugin/contact-form-7":"5.6",
        
    },
    "require-dev": {
        "wpackagist-plugin/simply-show-hooks":"*",
